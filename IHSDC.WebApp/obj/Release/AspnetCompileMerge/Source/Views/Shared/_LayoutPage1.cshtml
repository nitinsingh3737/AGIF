<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   
    <title>Customization of ASP.NET MVC DataTable using DataTable Editable plugin</title>
    <link href="@Url.Content("~/Content/dataTables1/dataTables/demo_table.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/dataTables1/dataTables/demo_table_jui.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/dataTables1/themes/base/jquery-ui.css")" rel="stylesheet" type="text/css" media="all" />
    <link href="@Url.Content("~/Content/dataTables1/themes/smoothness/jquery-ui-1.7.2.custom.css")" rel="stylesheet" type="text/css" media="all" />
    <script src="@Url.Content("~/Scripts/dataTables1/jquery-1.4.4.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/dataTables1/jquery.dataTables.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/dataTables1/jquery.jeditable.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/dataTables1/jquery-ui.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/dataTables1/jquery.validate.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/dataTables1/jquery.dataTables.editable.js")" type="text/javascript"></script>
    @RenderSection("head", required: false)
    <title>@ViewBag.Title - @IHSDC.Common.Configurations.Application.Name</title>
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-lg navbar-dark bg-primary fixed-top">
            @Html.ActionLink(IHSDC.Common.Configurations.Application.Abbreviation, "Index", "Home", new object { }, new { @class = "navbar-brand" })
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div id="navbarNavDropdown1" class="navbar-collapse collapse">
                @Html.Partial("_LoginPartial")
                <ul class="navbar-nav">
                    @Html.Partial("_DeveloperLinksPartial")
                </ul>
            </div>
        </nav>
    </header>
    <main class="display">
        <div class="row">
            
        </div>
        @RenderBody()
        <hr />
    </main>
    <footer class="container">
        <div class="row">
            <div class="col-md-3"><p class="small">&copy; @DateTime.Now.Year - @IHSDC.Common.Configurations.Application.Name</p></div>
            
        </div>
    </footer>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>

