
@{
    ViewBag.Title = "UploadITCERTData";
    Layout = "~/Views/Shared/_Layoutagi.cshtml";
}
<div class="container" style="background:white;padding:5%;">
    <div class="row">
        <div class="col-lg-12">
            @using (Html.BeginForm("upload", "ITCERT", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {

        @*@Html.AntiForgeryToken()*@
        @Html.ValidationSummary(true, "", new { @class = "text-danger" })

        <b style="font-size:20px;">ITCERT Data File</b>

        <div style="margin-top:3%;">
            @Html.TextBox("file", "", new { type = "file", required = "required", accept=".txt", @class = "form-control" })
            @ViewBag.Message

        </div>
        <div style="float:right;margin-top:2%;">
            <input type="submit" class="btn btn-success" name="Upload" value="Upload" />
        </div>

}
        </div>
        </div>
        </div>

