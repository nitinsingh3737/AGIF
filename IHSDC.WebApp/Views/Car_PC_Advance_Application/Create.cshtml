<script>
    function validateAmount(input) {
        var enteredAmount = parseFloat(input.value); // Get the value entered in the Amount_Applied_For_Loan field
        var eligibleAmount = parseFloat(document.getElementById("Amt_Eligible_for_loan").value); // Get the value set in the Amt_Eligible_for_loan field

        if (isNaN(enteredAmount)) {
            // If the entered value is not a number, set the field to empty
            input.value = '';
        } else {
            // If the entered value is a number, validate against the eligible amount
            if (enteredAmount > eligibleAmount) {
                // If the entered amount exceeds the eligible amount, set the field value to the eligible amount
                input.value = eligibleAmount;
            }
        }
    }
</script>

<script>
    function validateEMI(input) {
        var enteredEMI = parseFloat(input.value); // Get the value entered in the No_Of_EMI_Applied field
        var eligibleEMI = parseFloat(document.getElementById("EMI_Eligible_for_loan").value); // Get the value set in the EMI_Eligible_for_loan field

        if (isNaN(enteredEMI)) {
            // If the entered value is not a number, set the field to empty
            input.value = '';
        } else {
            // If the entered value is a number, validate against the eligible EMI
            if (enteredEMI > eligibleEMI) {
                // If the entered EMI exceeds the eligible EMI, set the field value to the eligible EMI
                input.value = eligibleEMI;
            }
        }
    }
</script>




<script>

    window.onload = function () {
        $('#Total_Cost').change(function () {

            var ResidualService = $('#Residual_Service').val();

            var Frequency = $('#FrequencyOfLoan').val();
            var vehicalType = $('#ApplicationType').val();
            var EMI = 0;
            /*var Residual = (ResidualService * 12) - 8;*/
            var Residual = Math.round((ResidualService * 12) - 8);

            if (Frequency == "1st Time" && (vehicalType == "Car" || vehicalType == "2")) {

                EMI = 96;
                if (EMI < Residual) {

                    document.getElementById("EMI_Eligible_for_loan").value = EMI;
                }
                else {

                    document.getElementById("EMI_Eligible_for_loan").value = Residual;
                }
            }
            else if (Frequency == "2nd Time" && (vehicalType == "Car" || vehicalType == "2")) {

                EMI = 72;
                if (EMI < Residual) {

                    document.getElementById("EMI_Eligible_for_loan").value = EMI;
                }
                else {

                    document.getElementById("EMI_Eligible_for_loan").value = Residual;
                }

            }
            else {

                EMI = 60;
                if (EMI < Residual) {

                    document.getElementById("EMI_Eligible_for_loan").value = EMI;
                }
                else {

                    document.getElementById("EMI_Eligible_for_loan").value = Residual;
                }

            }

        });



        $('#Total_Cost').change(function () {
            var prefix = $('#dropdownSelect').val();
            var CarType = $('#CarLoanType').val();
            var vehicalType = $('#ApplicationType').val();
            CostExShowroom = parseFloat($('#Total_Cost').val());
            var Amount = 0;
            var data = 0;

            debugger;
            var Amount;
            var data = Math.round(CostExShowroom * 0.9);

            if ((vehicalType == "Car" || vehicalType == "2")) {


                if (CarType == "New Car" || CarType == "5") {
                    Amount = (prefix == "JC" || prefix == "OR") ? 1000000 : 2000000;
                }
                else if (CarType == "Old Car" || CarType == "6") {
                    Amount = (prefix == "JC" || prefix == "OR") ? 500000 : 1000000;
                }
                else if (CarType == "EV" || CarType == "7") {
                    Amount = (prefix == "JC" || prefix == "OR") ? 1500000 : 2500000;
                }
            }
            else {
                if (vehicalType == "Two Wheeler Application" || vehicalType == "7") {
                    Amount = (prefix == "JC" || prefix == "OR") ? 200000 : 1000000;
                }
                //else if (CarType == "New Car" || CarType == "5") {
                //    Amount = 1000000;
                //}
            }


            if (Amount < data) {
                document.getElementById("Amt_Eligible_for_loan").value = Amount;
            } else {
                document.getElementById("Amt_Eligible_for_loan").value = data;
            }


        });



    };

