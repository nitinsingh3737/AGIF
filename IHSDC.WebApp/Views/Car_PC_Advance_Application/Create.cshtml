<script>
    function changeApp(event) {
        var selectedValue = event.target.value; // Get the selected value of Application Type dropdown
        var frequencyDropdown = document.getElementById('FrequencyOfLoan'); // Get the Frequency of Loan dropdown

        // Clear existing options
        frequencyDropdown.innerHTML = '';

        // Define options based on the selected application type
        if (selectedValue === '2') { // Car
            addOption(frequencyDropdown, '0', '--select--');
            addOption(frequencyDropdown, '1st Time', '1st Time');
            addOption(frequencyDropdown, '2nd Time', '2nd Time');

            $("#CarLoanType").prop('disabled', false);
            $("#Veh_Type").prop('disabled', false);
            // Add more options as needed
        } else if (selectedValue === '7') { // Two Wheeler
            $("#CarLoanType").prop('disabled', true);
            $("#Veh_Type").prop('disabled', true);
            addOption(frequencyDropdown, '0', '--select--');
            addOption(frequencyDropdown, '1st Time', '1st Time');
            addOption(frequencyDropdown, '2nd Time', '2nd Time');
            addOption(frequencyDropdown, '3rd Time', '3rd Time');
            // Add more options as needed
        }
    }

    function addOption(selectElement, value, text) {
        var option = document.createElement('option');
        option.value = value;
        option.text = text;
        selectElement.add(option);
    }

    function validateForm() {
        var frequencyDropdown = document.getElementById('FrequencyOfLoan');
        var selectedValue = frequencyDropdown.value;
        if (selectedValue === '0') {
            alert('Please select an option from the dropdown.');
            return false; // Prevent form submission
        }
        return true; // Allow form submission
    }
</script>



